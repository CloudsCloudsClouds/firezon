;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Proyecto Firezon"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[audio]

manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/pool_3D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"

[autoload]

VfxManager="*res://scripts/vfx_manager.gd"
SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=720
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Proyecto Firezon"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_visualstudio_debugger/plugin.cfg", "res://addons/proton_scatter/plugin.cfg", "res://addons/resonate/plugin.cfg", "res://addons/script-ide/plugin.cfg")

